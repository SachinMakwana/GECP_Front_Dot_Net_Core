@model Task<GECP.Models.Pages.BasicCellDetailsPageModel>

@{
    ViewBag.Title = "GrievenceCell";
}
<div class="page_title3">
    <div class="container">

        <h1>
            Grievence Cell <em>Government Engineering College</em>

        </h1>
        <div class="line"></div>
    </div>
</div>
<div class="clearfix"></div>

<div class="content_fullwidth">
    <div class="featured_section55 animate" data-anim-type="fadeIn" data-anim-delay="100">
        <div class="container">

            <h2 class="caps">About GECP Grievence</h2>

            <br /><br />

            <div class="one_third">

                <h4 class="color3 margin_bottom2">GECP Grievence Cell</h4>
                <p class="bigtfont">
                    @Model.Result.BasicCellDetailsModel.description
                </p>

            </div>
            <!-- end section -->

            <div class="one_third">

                <img src="@Model.Result.BasicCellDetailsModel.image" alt="" width="358.6" height="199.783">

            </div>
            <!-- end section -->

            <div class="one_third last">

                <h4 class="color3 margin_bottom2">GECP Measures</h4>
                <p class="bigtfont">
                    @Model.Result.BasicCellDetailsModel.measure
                </p>

            </div>
            <!-- end section -->

        </div>
    </div>
</div>
<div class="margin_top2"></div>
<div class="featured_section14">
    <div class="container">

        <div class="one_half animate" data-anim-type="fadeInLeft" data-anim-delay="300">

            <img src="~/images/icon-img13.png" alt="" />
            <br /><br />
            <h3 class="white">Vision</h3>

            <p>@Model.Result.vissionMissionModel.Vision</p>
            <br /><br />


        </div>
        <!-- end section -->

        <div class="one_half last animate" data-anim-type="fadeInRight" data-anim-delay="300">

            <img src="~/images/icon-img12.png" alt="" />
            <br /><br />
            <h3 class="white">Mission</h3>

            <p>@Model.Result.vissionMissionModel.Mission</p>
            <br /><br />


        </div>
        <!-- end section -->

    </div>
</div>
<!-- end featured section 14 -->

<div class="featured_section8 animate" data-anim-type="fadeIn" data-anim-delay="200">

    <div class="puhtext">
        <div class="logregform two">
            <div class="feildcont">

                <form role="form">

                    <label>Name <em>*</em></label>
                    <input type="text">
                    <div class="one_half">
                        <label>Email <em>*</em></label>
                        <input type="text">
                    </div>
                    <div class="one_half last">
                        <label>Mobile No <em>*</em></label>
                        <input type="text">
                    </div>

                    <div class="one_half">
                        <label>Department</label>

                        <select>
                            <option value="0">- Select -</option>
                            <option value="usa">EC</option>
                            <option value="india">Computer</option>
                            <option value="china">Electrical</option>
                            <option value="uk">Civil</option>
                            <option value="australia">Mechanical</option>


                        </select>
                    </div>

                    <div class="one_half last">
                        <label>Enrollment<em>*</em></label>
                        <input type="password">
                    </div>


                    <div class="one_third radiobut">
                        <label>Gender</label>
                        <input class="one" type="radio" name="sex" value="male" checked>
                        <span class="onelb">Male</span>
                        <input class="two" type="radio" name="sex" value="female">
                        <span class="onelb">Female</span>
                    </div>


                    <label>Complaint Message</label>
                    <input type="textbox">



                    <!-- <div class="checkbox">
                        <label>
                            <input type="checkbox">
                        </label>
                        <label>I agree the User Agreement and<a href="#">Terms &amp; Condition.</a></label>
                    </div> -->

                    <a href="#" class="fbut">Send Message</a>


                </form>

            </div>
        </div>

        <!-- <div class="hsmline3 margin_top4 animate" data-anim-type="fadeInRight" data-anim-delay="500"></div> -->

    </div>

</div>
<!-- end featured section 8 -->


<div class="featured_section19">
    <div class="container">

        <div class="who animate" data-anim-type="fadeInLeft" data-anim-delay="400"><img src="~/images/site-img16.png" alt="" /></div>

        <div class="text animate" data-anim-type="fadeIn" data-anim-delay="500">

            <h1>Grievence Head Message</h1>

            <p>
                @Model.Result.MsgCmtModel.Message
            </p>

            <h2>Darin Matthew.</h2>

        </div>

    </div>
</div>
<!-- end featured section 19 -->

