        </div>
        <!-- end section -->

        <div class="one_fourth animate" data-anim-type="fadeInRight" data-anim-delay="100">

            <img src="~/images/peop-img39.jpg" alt="" />
            <h5 class="roboto caps"><a href="#"><strong>Gaige Leroy</strong></a></h5>
            <p>SupportingMember</p>


        </div>
        <!-- end section -->

        <div class="one_fourth last animate" data-anim-type="fadeInRight" data-anim-delay="300">

