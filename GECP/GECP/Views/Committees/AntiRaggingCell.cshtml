<div class="works01">
    <div class="container">

        <div id="filters-container" class="cbp-l-filters-button">
            <div data-filter="*" class="cbp-filter-item-active cbp-filter-item">
                All
                <div class="cbp-filter-counter"></div>
            </div>

            @foreach (var item in Model.Result.CategoryModels)
            {
                <div data-filter=.@item.categoryClass class="cbp-filter-item">
                    @item.category
                    <div class="cbp-filter-counter"></div>
                </div>
            }
        </div>


        <div id="grid-container" class="cbp-l-grid-fullWidth">
            @foreach (var item in Model.Result.GalleryModels)
            {
                <div class="cbp-item @item.categoryClass logos">
                    <div class="cbp-caption">
                        <div class="cbp-caption-defaultWrap">
                            <img src=@item.image alt="">
                        </div>
                        <div class="cbp-caption-activeWrap">
                            <div class="cbp-l-caption-alignCenter">
                                <div class="cbp-l-caption-body">
                                    <h4>@item.title</h4>
                                    <a href=@item.image class="cbp-lightbox cbp-l-caption-buttonRight" data-title=@item.title>view larger</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }

        </div>

        <!--<div class="cbp-l-loadMore-text">
        <div data-href="ajax/loadMore.html" class="cbp-l-loadMore-text-link">LOADING...</div>
    </div>-->


    </div>
</div>
